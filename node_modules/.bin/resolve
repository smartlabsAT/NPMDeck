#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/private/var/www/nginx-proxy-manager/frontend-new/node_modules/resolve/bin/node_modules:/private/var/www/nginx-proxy-manager/frontend-new/node_modules/resolve/node_modules:/private/var/www/nginx-proxy-manager/frontend-new/node_modules:/private/var/www/nginx-proxy-manager/node_modules:/private/var/www/node_modules:/private/var/node_modules:/private/node_modules:/node_modules:/private/var/www/nginx-proxy-manager/frontend-new/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/private/var/www/nginx-proxy-manager/frontend-new/node_modules/resolve/bin/node_modules:/private/var/www/nginx-proxy-manager/frontend-new/node_modules/resolve/node_modules:/private/var/www/nginx-proxy-manager/frontend-new/node_modules:/private/var/www/nginx-proxy-manager/node_modules:/private/var/www/node_modules:/private/var/node_modules:/private/node_modules:/node_modules:/private/var/www/nginx-proxy-manager/frontend-new/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../resolve/bin/resolve" "$@"
else
  exec node  "$basedir/../resolve/bin/resolve" "$@"
fi
