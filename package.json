{
  "name": "npmDeck",
  "version": "0.1.3",
  "description": "Modern frontend for Nginx Proxy Manager",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"pnpm run dev:server\" \"pnpm run dev:client\"",
    "dev:server": "nodemon server/index.js",
    "dev:client": "vite --host 0.0.0.0",
    "build": "tsc && vite build",
    "start": "NODE_ENV=production node server/index.js",
    "start:dev": "pnpm run dev",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives",
    "lint:strict": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "typecheck": "tsc --noEmit",
    "docker:build": "docker build -t npmdeck:latest .",
    "docker:run": "docker run -p 3000:3000 npmdeck:latest"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^5.18.0",
    "@mui/material": "^5.18.0",
    "@mui/utils": "^7.3.2",
    "@mui/x-date-pickers": "^8.11.2",
    "@tanstack/react-query": "^5.87.4",
    "@types/react-syntax-highlighter": "^15.5.13",
    "axios": "^1.12.2",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.1",
    "helmet": "^7.2.0",
    "http-proxy-middleware": "^2.0.9",
    "morgan": "^1.10.1",
    "prismjs": "^1.30.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.3.8",
    "react-router-dom": "^6.30.1",
    "react-simple-code-editor": "^0.14.1",
    "react-syntax-highlighter": "^15.6.6",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@types/node": "^24.4.0",
    "@types/prismjs": "^1.26.5",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@vitejs/plugin-react": "^4.7.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.20",
    "nodemon": "^3.1.10",
    "typescript": "^5.9.2",
    "vite": "^5.4.19"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/npmDeck.git"
  },
  "keywords": [
    "nginx",
    "proxy",
    "reverse-proxy",
    "docker",
    "react",
    "frontend"
  ],
  "author": "Your Name",
  "license": "MIT"
}