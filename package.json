{
  "name": "npmDeck",
  "version": "0.1.4",
  "description": "Modern frontend for Nginx Proxy Manager",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"pnpm run dev:server\" \"pnpm run dev:client\"",
    "dev:server": "nodemon server/index.js",
    "dev:client": "vite --host 0.0.0.0",
    "build": "tsc && vite build",
    "start": "NODE_ENV=production node server/index.js",
    "start:dev": "pnpm run dev",
    "preview": "vite preview",
    "lint": "eslint --report-unused-disable-directives",
    "lint:strict": "eslint --report-unused-disable-directives --max-warnings 0",
    "typecheck": "tsc --noEmit",
    "docker:build": "docker build -t npmdeck:latest .",
    "docker:run": "docker run -p 3000:3000 npmdeck:latest"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^5.18.0",
    "@mui/material": "^5.18.0",
    "@mui/utils": "^7.3.7",
    "@mui/x-date-pickers": "^8.27.0",
    "@tanstack/react-query": "^5.90.20",
    "@types/react-syntax-highlighter": "^15.5.13",
    "axios": "^1.13.5",
    "cors": "^2.8.6",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.4",
    "express": "^5.2.1",
    "express-rate-limit": "^7.5.1",
    "helmet": "^8.1.0",
    "http-proxy-middleware": "^3.0.5",
    "morgan": "^1.10.1",
    "prismjs": "^1.30.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.4.0",
    "react-router-dom": "^7.13.0",
    "react-simple-code-editor": "^0.14.1",
    "react-syntax-highlighter": "^16.1.0",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^24.4.0",
    "@types/prismjs": "^1.26.5",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^5.1.3",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.5.0",
    "globals": "^17.3.0",
    "nodemon": "^3.1.11",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0",
    "vite": "^7.3.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/npmDeck.git"
  },
  "keywords": [
    "nginx",
    "proxy",
    "reverse-proxy",
    "docker",
    "react",
    "frontend"
  ],
  "author": "Your Name",
  "license": "MIT",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ],
    "allowedDeprecatedVersions": {}
  }
}
