{
  "name": "npmDeck",
  "version": "1.0.0",
  "description": "Modern frontend for Nginx Proxy Manager",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"pnpm run dev:server\" \"pnpm run dev:client\"",
    "dev:server": "nodemon server/index.js",
    "dev:client": "vite --host 0.0.0.0",
    "build": "tsc && vite build",
    "start": "NODE_ENV=production node server/index.js",
    "start:dev": "pnpm run dev",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "typecheck": "tsc --noEmit",
    "docker:build": "docker build -t npmdeck:latest .",
    "docker:run": "docker run -p 3000:3000 npmdeck:latest"
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.0",
    "@mui/material": "^5.14.0",
    "@mui/x-date-pickers": "^8.9.2",
    "@tanstack/react-query": "^5.0.0",
    "@types/prismjs": "^1.26.5",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "http-proxy-middleware": "^2.0.6",
    "morgan": "^1.10.0",
    "prismjs": "^1.30.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.3.8",
    "react-router-dom": "^6.20.0",
    "react-simple-code-editor": "^0.14.1",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@types/node": "^24.1.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.0",
    "nodemon": "^3.0.2",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/npmDeck.git"
  },
  "keywords": [
    "nginx",
    "proxy",
    "reverse-proxy",
    "docker",
    "react",
    "frontend"
  ],
  "author": "Your Name",
  "license": "MIT"
}